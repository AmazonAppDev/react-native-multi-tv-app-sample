# Amazon GameLift Streams Configuration
# Copy this file to .env and replace with actual values

# AWS Cognito Authentication
GAMELIFT_USER_POOL_ID=us-west-2_XXXXXXXXX
GAMELIFT_USER_POOL_CLIENT_ID=XXXXXXXXXXXXXXXXXX
GAMELIFT_COGNITO_REGION=us-west-2

# GameLift Streams API
GAMELIFT_API_ENDPOINT=https://XXXXXXXXXX.execute-api.us-west-2.amazonaws.com/prod
GAMELIFT_API_REGION=us-west-2

# GameLift Streams Configuration
GAMELIFT_DEFAULT_REGIONS=us-west-2,us-east-1,eu-west-1
GAMELIFT_CONNECTION_TIMEOUT=120
GAMELIFT_MAX_POLLING_TIME=600000
